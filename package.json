    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/instrument-cra": "^1.4.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/parser": "^5.45.0",
    "babel-plugin-jsx-remove-data-test-id": "^2.1.3",
    "concurrently": "^7.0.0",
    "cypress": "5.4.0",
    "dynamic-cdn-webpack-plugin": "^5.0.0",
    "eslint": "^8.10.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "http-proxy-middleware": "^1.0.5",
    "husky": "^4.2.5",
    "istanbul-lib-coverage": "^3.0.0",
    "lint-staged": "^10.1.3",
    "nyc": "^15.0.1",
    "poeditor-ci": "^1.15.0",
    "prettier": "^2.1.1",
    "react-scripts": "5.0.1",
    "sass": "^1.32.12",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.9.3",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "scripts": {
    "start": "concurrently -r --kill-others 'yarn start:server' 'yarn start:services'",
    "start:server": "react-scripts -r @cypress/instrument-cra start",
    "start:services": "docker-compose up",
    "build": "REACT_APP_VERSION=$npm_package_version react-scripts build",
    "prettier": "prettier --write \"src/**/*.js\"",
    "lint": "eslint src/ --fix",
    "fix": "yarn prettier && yarn lint",
    "patch": "yarn version --no-git-tag-version --patch",
    "eject": "react-scripts eject",
    "test": "yarn test:e2e && yarn test:unit",
    "test:e2e": "BROWSER=none PORT=3030 start-test 'yarn start:server' http://127.0.0.1:3030/ 'cypress run'",
    "test:unit": "react-scripts test",
    "test:coverage": "yarn test:unit --watchAll=false --coverage",
    "typecheck": "yarn run tsc",
    "cypress": "BROWSER=none PORT=3030 start-test 'yarn start:server' http://127.0.0.1:3030/ 'cypress open'",
    "translations:fetch": "poeditor pull",
    "translations:update": "poeditor push"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,tsx}": [
      "eslint --fix"
    ],
    "cypress/**/*": [
      "yarn test"
    ]
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "nyc": {
    "reporter": [
      "html"
    ]
  }
}
