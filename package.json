    "i18next": "^14.0.1",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^2.0.1",
    "localforage": "^1.7.4",
    "lodash": "^4.17.13",
    "md5": "^2.2.1",
    "node-sass": "^4.13.1",
    "prettier": "^2.1.1",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "react": "^16.13.1",
    "react-day-picker": "^7.1.10",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-i18next": "^10.6.1",
    "react-lazy-load-image-component": "^1.3.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-timekeeper": "^2.1.3",
    "reactstrap": "8.9.0",
