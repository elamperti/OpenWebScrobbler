    "bootswatch": "^5.1.3",
    "date-fns": "^1.29.0",
    "history": "^5.3.0",
    "i18next": "^14.0.1",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^2.0.1",
    "localforage": "^1.7.4",
    "lodash": "^4.17.13",
    "md5": "^2.2.1",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "react": "^18.2.0",
    "react-day-picker": "^7.4.9",
    "react-dom": "^18.2.0",
    "react-ga": "^3.1.2",
    "react-i18next": "^11.15.5",
    "react-lazy-load-image-component": "^1.5.4",
    "react-redux": "^7.2.0",
    "react-router-dom": "^6.2.2",
