    "react-scripts": "3.4.1",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "scripts": {
    "start": "concurrently -r --kill-others 'react-app-rewired -r @cypress/instrument-cra start' 'docker-compose up'",
    "build": "REACT_APP_VERSION=$npm_package_version react-app-rewired build",
    "lint": "node_modules/.bin/eslint -c .eslintrc src/**/*.js",
    "patch": "yarn version --no-git-tag-version --patch",
    "eject": "react-scripts eject",
    "test": "cypress run",
    "cypress": "cypress open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "concurrent": false,
    "linters": {
      "src/**/*.{js,jsx}": [
        "node_modules/.bin/eslint"
      ]
    }
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "proxy": "http://localhost:3274"
